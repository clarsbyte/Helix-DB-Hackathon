// Schema for a notes system with vector embeddings

// Regular node to store note metadata
N::Note {
    title: String,
    content: String,
    created_at: String,
    INDEX note_id: I32
}

// Vector node to store embeddings of notes for semantic search
V::NoteEmbedding {
    title: String,
    content: String,
    note_id: I32
}

// Edge connecting a note to its embedding
E::HasEmbedding {
    From: Note,
    To: NoteEmbedding
}
